@{
    ViewBag.Title = "Index";
}

<table class="table">
    <tr>
        <td>
            <div class="header">
                <h3 class="title">Generar Servicios de Planes de Salud</h3>
            </div>
        </td>
    </tr>
    <tr>
        <td>
            <div class="content">
                @{Html.BeginForm("Generar", "GenerarServiciosPlan", FormMethod.Post, new { @class = "some_class", @id = "form_generar_servicio_salud" });}
                <div class="col-md-6">
                    <div class="form-group">
                        <label>Fecha inicio</label>
                        <input id="fecha_inicio" name="fecha_inicio" type="date" class="form-control datepicker" placeholder="Fecha inicio" value="01/01/2015">
                    </div>
                </div>
            </div>
        </td>
        <td>
            <div class="col-md-6">
                <div class="form-group">
                    <label>Fecha Fin</label>
                    <input id="fecha_fin" name="fecha_fin" type="date" class="form-control datepicker" placeholder="Fecha fin" value="30/12/2017">
                </div>
            </div>
        </td>
        </tr>
    <tr>
        <td>
            <div class="col-md-6">
                <div class="form-group">
                    <input id="submit_generar_servicio_salud" name="submit_generar_servicio_salud" value="Generar Servicio de Salud" type="submit" class="btn btn-info btn-fill pull-left" />
                </div>
            </div>
        </td>
    </tr>
    <tr>
        <td>
            @{Html.EndForm();}
        </td>
            <br />

            @ViewBag.Message

            <div class="clearfix"></div>

        </td>
    </tr>
</table>
